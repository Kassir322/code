{
  "kind": "collectionType",
  "collectionName": "order_items",
  "info": {
    "singularName": "order-item",
    "pluralName": "order-items",
    "displayName": "OrderItem"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "quantity": {
      "type": "integer",
      "required": true,
      "default": 1
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "order": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::order.order"
    },
    "study_card": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::study-card.study-card"
    }
  }
}
