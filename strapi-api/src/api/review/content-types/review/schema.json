{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "singularName": "review",
    "pluralName": "reviews",
    "displayName": "Review"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "rating": {
      "type": "integer",
      "required": true,
      "min": 1,
      "max": 5
    },
    "comment": {
      "type": "text"
    },
    "study_card": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::study-card.study-card"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
